{
	"name": "clipboardhacker",
	"displayName": "ClipboardHacker",
	"description": "",
	"version": "0.0.2",
	"engines": {
		"node": "^12.4.0",
		"vscode": "^1.30.0"
	},
	"publisher": "to-lz1",
	"license": "MIT",
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"repository": {
    "type": "git",
    "url": "git@github.com:to-lz1/clipboardhacker.git"
  },
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "cliphack.cut-with-replace",
				"title": "Cut with Replace"
				},
			{
				"command": "cliphack.copy-with-replace",
				"title": "Copy with Replace"
			}],
		"configuration": {
			"type": "object",
			"title": "Clipboard Hack Configuration",
			"properties": {
				"cliphack.dictionary": {
						"type": "array",
						"items": {
							"type": "object",
							"title": "inner objects",
							"properties": {
								"from": {
									"type": "string",
									"description": "replace target regexp"
								},
								"to": {
									"type": "string",
									"description": "replaced string will be this string"
								}
							}
						},
						"default": [],
						"description": "cliphack replace dictionary"
				}
			}
		}
},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "yarn run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	}
}
